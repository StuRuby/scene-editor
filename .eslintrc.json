{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "plugins": [
        "prettier"
    ],
    "root": true,
    "rules": {
        "prettier/prettier": [
            "warn",
            {},
            {
                "usePrettierrc": false
            }
        ]
    }
}